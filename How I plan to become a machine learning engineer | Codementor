.katex .accent .accent-body.accent-vec > span { position: relative; left: 0.326em; }

.katex .mtable .vertical-separator { display: inline-block; margin: 0px -0.025em; border-right: 0.05em solid rgb(0, 0, 0); }

.katex .mtable .arraycolsep { display: inline-block; }

.katex .mtable .col-align-c > .vlist { text-align: center; }

.katex .mtable .col-align-l > .vlist { text-align: left; }

.katex .mtable .col-align-r > .vlist { text-align: right; }
